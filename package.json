    "postcss-loader": "^0.13.0",
    "postcss-normalize-charset": "latest",
    "progress-bar-webpack-plugin": "^1.9.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.3.0",
    "stylelint-loader": "^6.2.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "write-file-webpack-plugin": "^3.3.0"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.15.0",
    "ghooks": "^1.3.1",
    "standard-version": "^2.4.0",
    "validate-commit-msg": "^2.8.0"
  },
  "peerDependencies": {
    "brickyard3": "^2.0.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "example",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 72,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "\n# allowed type: feat, fix, docs, style, refactor, example, perf, test, chore, revert\n# subject no more than 50 chars\n# a body line no more than 72 chars"
    },
    "ghooks": {
      "//": {
        "post-checkout": "npm install"
      },
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg",
      "post-rewrite": "npm install",
      "post-merge": "npm install"
    }
  }
}

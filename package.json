  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prerelease": "npm run lint",
    "release": "standard-version",
    "push": "git push --follow-tags && npm publish . --registry=https://registry.npmjs.org/"
  },
  "author": "e-cloud",
